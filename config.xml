<?xml version='1.0' encoding='utf-8'?>
<widget id="org.fathens.TritonNote" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TritonNote</name>
    <description>
        Triton Note Client
    </description>
    <author email="admin+triton_note@fathens.org" href="http://www.fathens.org/">
        Office fathens
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="deployment-target" value="8.0" />
    <preference name="SplashScreenDelay" value="30000" />
    <preference name="AutoHideSplashScreen" value="false" />

    <engine name="android" spec="~4.1.1" />
    <engine name="ios" spec="~3.9.2" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~1.3.1" />
    <plugin name="cordova-plugin-device" spec="~1.0.1" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-camera" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~2.1.0" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-geolocation" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.0.0" />
    <plugin name="phonegap-plugin-push" spec="~1.3.0" />
    <plugin name="org.apache.cordova.file" spec="https://github.com/sawatani/Cordova-plugin-file.git#GooglePhotos" />
    <plugin name="org.fathens.cordova.plugin.fabric.Crashlytics" spec="https://github.com/fathens/Cordova-Plugin-Crashlytics.git" />
</widget>
